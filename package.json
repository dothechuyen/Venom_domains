  "scripts": {
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "dev": "tsc && tsc -b && npx nodemon -r ts-node/register/transpile-only -r tsconfig-paths/register dist/server.js",
    "build": "tsc && tsc -b",
    "start": "tsc && tsc -b && node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/server.js",
    "migrate:create": "typeorm migration:create -n",
    "migrate:run": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/migration.js up",
    "migrate:down": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/migration.js down"
  },
